{
  "description": "Configure external DB2",
  "name": "DB2ConfigExternal",
  "type": "userCreated",
  "manifest": {
    "template_type": "Terraform",
    "template_provider": "Other",
    "icon": "serviceicon_4.svg",
    "template": {
      "templateData": "variable \"host\" {\n  description = \"IP of host to ssh\"\n}\n\nvariable \"root_password\" {\n  default = \"passw0rd\"\n  description = \"ssh root password\"\n}\nvariable \"db_password\" {\n  default = \"passw0rd\"\n  description = \"db password\"\n}\nvariable \"dbname\" {\n  default = \"trader\"\n  description = \"database name\"\n}\nvariable \"db_user\" {\n  default = \"db2inst1\"\n  description = \"database user\"\n}\nresource \"null_resource\" \"DB_Config1\" {\n  provisioner \"remote-exec\" {\n      inline = [\n         \"chmod +x /tmp/DB2_Sript.sh\",\n         \"su - db2inst1 -c \\\"sh /tmp/DB2_Sript.sh ${var.dbname} ${var.db_user} ${var.db_password}\\\"\",\n      ]\n  connection {\n    host = \"${var.host}\"\n    type     = \"ssh\"\n    user     = \"root\"\n    password = \"${var.root_password}\"\n  }\n }\n}\n",
      "templateVariables": {
        "template_input_params": [
          {
            "name": "db_password",
            "description": "db password",
            "default": "",
            "type": "password",
            "required": true,
            "secured": true,
            "label": "db_password",
            "hidden": false,
            "immutable": false
          },
          {
            "name": "db_user",
            "description": "database user",
            "default": "db2inst1",
            "type": "string",
            "required": true,
            "secured": false,
            "label": "db_user",
            "hidden": false,
            "immutable": false
          },
          {
            "name": "dbname",
            "description": "database name",
            "default": "trader",
            "type": "string",
            "required": true,
            "secured": false,
            "label": "dbname",
            "hidden": false,
            "immutable": false
          },
          {
            "name": "host",
            "description": "IP of host to ssh",
            "type": "string",
            "required": true,
            "secured": false,
            "label": "host",
            "hidden": false,
            "immutable": false
          },
          {
            "name": "root_password",
            "description": "ssh root password",
            "default": "",
            "type": "password",
            "required": true,
            "secured": true,
            "label": "root_password",
            "hidden": false,
